{
  "name": "tier-analysis-app",
  "productName": "Cascading Failure Tier Analysis",
  "version": "0.0.1",
  "description": "A graphical app wrapped around the byuboydlab/tier_analysis repo",
  "main": "src/main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "build": "electron-builder"
  },
  "build": {
    "appId": "io.github.byuboydlab.tier-analysis-app",
    "files": [
      "src/*",
      "!src/executables",
      "!src/icon",
      "!src/python"
    ],
    "extraFiles": [
      {
        "from": "src/executables/tier_analysis.exe",
        "to": "executables/tier_analysis.exe"
      }
    ],
    "icon": "src/icon/boyd_logo_rounded_corners.png"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/byuboydlab/tier-analysis-app.git"
  },
  "author": "Isaac Elmer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/byuboydlab/tier-analysis-app/issues"
  },
  "homepage": "https://github.com/byuboydlab/tier-analysis-app#readme",
  "devDependencies": {
    "electron": "^35.0.1",
    "electron-builder": "^26.0.12"
  },
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "tree-kill": "^1.2.2"
  }
}
